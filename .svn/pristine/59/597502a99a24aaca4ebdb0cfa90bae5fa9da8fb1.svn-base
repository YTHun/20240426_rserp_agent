	<!-- INSERT -->
	<insert id="ifAutoPay_Dao.insertAutoPay.insert" parameterType="CmMap">
		INSERT INTO apart.if_autopay(
					comp_gb
		  ,   dancode
		  ,   dongcode
		  ,   roomno
		  ,   gojicode
		  ,   accountno
		  ,   admincode
		  ,   bankname
		  ,   depositname
