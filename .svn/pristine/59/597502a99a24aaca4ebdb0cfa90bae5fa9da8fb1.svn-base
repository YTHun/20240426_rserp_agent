		  ,   row_input_ip
		  ,   row_update_date
		  ,   row_update_emp_no
		  ,   row_update_ip
		)
		VALUES(
					#{company_code}
		  ,   #{dangi_code}
		  ,   #{dong_code}
		  ,   #{room_no}
		  ,   #{goji_code}
		  ,   #{account_no}
		  ,   #{admin_code}
		  ,   #{bank_name}
		  ,   #{deposit_name}
		  ,   #{rec_kind}
		  ,   #{rec_bank}
		  ,   #{req_date}
		  ,   #{req_kind}
		  ,   now()
		  ,   '오정현'
		  ,   '1.0.0.0.0.0'
		  ,   now()
			,   '오정현'
			,   '1.0.0.0.0.0'
		);
